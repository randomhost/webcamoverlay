{
    "name": "randomhost/webcamoverlay",
    "type": "library",
    "description": "Provides image overlay functionality for IP cam images.",
    "keywords": [
        "Image",
        "Webcam"
    ],
    "homepage": "https://github.random-host.tv",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ch'Ih-Yu",
            "email": "chi-yu@web.de"
        }
    ],
    "config": {
        "platform": {
            "php": "7.3.31"
        }
    },
    "require": {
        "php": ">=7.3.0",
        "randomhost/image": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0"
    },
    "autoload": {
        "psr-4": {
            "randomhost\\Image\\Webcam\\": "src/php/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "randomhost\\Image\\Webcam\\Tests\\": "src/tests/php"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
